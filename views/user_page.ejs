  <!--this is a pop up icon for login and signup-->
  <div id="login" class="form-popup" style="margin-left:30%;margin-right:15%;margin-top:25%;width:500px;height:auto;background-color:white;display:none;">
    <form action="/login" method="post">
      <div style="width:auto;margin-left:20%;margin-right:20%; margin-top:30px" class="input-group input-group-sm mb-3">
        <div class="input-group-prepend">
          <span class="input-group-text" id="inputGroup-sizing-sm">Email</span>
        </div>
        <input type="email" name="email" class="form-control" aria-label="Small" aria-describedby="inputGroup-sizing-sm" required>
      </div>
      <div style="width:auto;margin-left:20%;margin-right:20%; margin-top:30px" class="input-group input-group-sm mb-3">
        <div class="input-group-prepend">
          <span class="input-group-text" id="inputGroup-sizing-sm">Password</span>
          <input id="password" type="password" name="password" class="form-control" aria-label="Small" aria-describedby="inputGroup-sizing-sm" required>
      </div>
        </div>
        <div style="margin:auto;width:auto;margin-left:25%;margin-right:20%; margin-top:30px;margin-bottom:30px;" class="form-check">
          <a id="show_password"> <u>Show Password</u></a>
        </div>
        
      <div style="text-align:center" >
      <button type="submit" class="btn btn-dark">LogIn</button>
      <div style="margin-top:30px;">
      <a style="margin-right:20px" href="/signup">click here to signup</a>
      <a href="/reset_password">forgot password</a>
    </div>
    </div>
    </form>
    
  </div>

<!--the pop up icon ends here-->
<script>
  function login()
  { 
    if(document.getElementById("login").style.display=="none")
   {
     document.getElementById("login").style.display="block";
     document.getElementById("login/signup").innerHTML="close form";
   }
   else if(document.getElementById("login").style.display=="block")
   {
     document.getElementById("login").style.display="none";
     document.getElementById("login/signup").innerHTML="login/signup";
   }
  }
  
  function login_first()
  {
    alert("please login first");
  }
</script>
  















